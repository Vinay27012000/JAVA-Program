package java;
import java.util.Scanner;
 class Room {

	
	int roomno;
	String roomtype;
	float roomarea;
	String acmachine;
	
void setdata()
{
Scanner s=new Scanner(System.in);
System.out.println("enter details \nroomno: \nroomtype: \nroomarea: \nacmachine");
roomno=s.nextInt();
roomtype=s.next();
roomarea=s.nextFloat();
acmachine=s.next();
			
}
void display()
{
	System.out.println(roomno +" "+ roomtype +" "+ roomarea +" "+ acmachine);
}
	
}
  class Main
{
public static void main(String[] args)
{
	Room r1=new Room();
	r1.setdata();
	r1.display();
}
}
