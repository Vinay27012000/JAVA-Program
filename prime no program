package ayush;

import java.util.Scanner;

public class Prime {
public static void main(String[] args)
{
	int n,i,temp=0;
	System.out.println( "ener a number ");
	Scanner sc = new Scanner(System.in);
    n=sc.nextInt();
    for(i=2;i<n/2;i++)
    {
    	if(n%i==0)
    	{
    		temp=1;
    	}	
    }
    if(temp==0)  
    {
    	System.out.println(" number is prime ");
    } 
    else
    {
    	System.out.println(" number is not prime ");
    }
  }
}
